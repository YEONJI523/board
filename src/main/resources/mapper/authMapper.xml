<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"
        >

<mapper namespace="com.project2.board.mapper.AuthMapper">
    <select id="findByAuth" parameterType="Integer" resultType="com.project2.board.dto.Auth">
        SELECT
            *
         FROM tb_user_auth
         WHERE user_id =#{user_id}

    </select>


</mapper>